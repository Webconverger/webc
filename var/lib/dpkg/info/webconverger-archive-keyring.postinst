#!/bin/sh

set -e

case "${1}" in
	configure)
		if [ -x /usr/bin/apt-key ]
		then
			/usr/bin/apt-key add /usr/share/keyrings/webconverger-archive-keyring.gpg
		fi
		;;

	abort-upgrade|abort-remove|abort-deconfigure)

		;;

	*)
		echo "postinst called with unknown argument \`${1}'" >&2
		exit 1
		;;
esac



exit 0
