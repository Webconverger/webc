	      <menuitem label="&toolbarbutton.change.directory;" oncommand="FoxSaver.preferenceWindow.selectPictureDirectory()"/>
        <menuitem oncommand="FoxSaver.fs.openOptionsDialog();" label="&toolbarbutton.options;" id="foxsaver-openprefs" />
