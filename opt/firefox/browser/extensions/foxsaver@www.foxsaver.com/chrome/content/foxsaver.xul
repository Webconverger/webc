  <statusbar id="status-bar">
    <statusbarpanel id="FoxSaverStatusBarIcon" context="FoxSaverStatusContextMenu" label="&toolbarbutton.slideshow;" onclick="FoxSaver.fs.onStatusBarIconClick(event);">
      <image id="foxsaver_statusbar_icon" tooltiptext="&toolbarbutton.slideshow;" />
    </statusbarpanel>
    <menupopup id="FoxSaverStatusContextMenu">
      <menuitem id="foxsaver_statusbar_menu_start" label="&toolbarbutton.start.slideshow;" oncommand="FoxSaver.fs.start();" />
      <menuitem id="foxsaver_statusbar_menu_start_ordered" label="&toolbarbutton.start.slideshow_ordered;" oncommand="FoxSaver.fs.start(true);" />
      <menuitem id="foxsaver_statusbar_menu_toggle" label="&toolbarbutton.turnoff;" type="checkbox" oncommand="FoxSaver.fs.toggle(event);" autocheck="false" />
      <menuseparator/>
		    <menu id="foxsaver_statusbar_menu_idle_minutes" label="&toolbarbutton.idle_minutes;">
		      <menupopup onpopupshowing="FoxSaver.preferenceWindow.setSelectedIdleMinutes(event);">
		        <menuitem id="foxsaver_statusbar_menu_idle_minutes_3"  label="3"  oncommand="FoxSaver.preference.setIdleMinutes(3);"  type="checkbox" autocheck="false"/>
		        <menuitem id="foxsaver_statusbar_menu_idle_minutes_5"  label="5"  oncommand="FoxSaver.preference.setIdleMinutes(5);"  type="checkbox" autocheck="false"/>
		        <menuitem id="foxsaver_statusbar_menu_idle_minutes_10" label="10" oncommand="FoxSaver.preference.setIdleMinutes(10);" type="checkbox" autocheck="false"/>
		        <menuitem id="foxsaver_statusbar_menu_idle_minutes_15" label="15" oncommand="FoxSaver.preference.setIdleMinutes(15);" type="checkbox" autocheck="false"/>
		        <menuitem id="foxsaver_statusbar_menu_idle_minutes_30" label="30" oncommand="FoxSaver.preference.setIdleMinutes(30);" type="checkbox" autocheck="false"/>
		        <menuitem id="foxsaver_statusbar_menu_idle_minutes_45" label="45" oncommand="FoxSaver.preference.setIdleMinutes(45);" type="checkbox" autocheck="false"/>
		        <menuitem id="foxsaver_statusbar_menu_idle_minutes_60" label="60" oncommand="FoxSaver.preference.setIdleMinutes(60);" type="checkbox" autocheck="false"/>
		      </menupopup>
		    </menu>
      <menuseparator/>
      <menuitem id="foxsaver_statusbar_menu_choose_directory" label="&toolbarbutton.change.directory;" oncommand="FoxSaver.preferenceWindow.selectPictureDirectory()"/>
      <menuitem id="foxsaver_statusbar_menu_options" oncommand="FoxSaver.fs.openOptionsDialog();" label="&toolbarbutton.options;" />
      <menuitem id="foxsaver_statusbar_menu_login" oncommand="FoxSaver.fs.openLoginDialog();" label="&toolbarbutton.login;" hidden="true"/>
      <menuseparator/>
      <menuitem id='foxsaver_statusbar_menu_about' label="&toolbarbutton.about;" oncommand="FoxSaver.Util.newTab('http://www.foxsaver.com');"/>
